<ShowImageScreen>:
    name: 'show_image'
    obj_image: imagetag
    pos_x: 500

    canvas.before:
        Color:
            rgb: 0, 0, 0
        Rectangle:
            pos: self.pos
            size: self.size

    FloatLayout:
        Image:
            id: imagetag
            source: 'resources/empty.jpg'

